{
  "title": "System Metrics",
  "tags": ["system", "metrics"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "CPU Usage",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m]) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Go Routines",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "go_goroutines",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"}
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}
